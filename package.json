  "bugs": {
    "url": "https://github.com/styzhang/design-colors/issues"
  },
  "homepage": "https://github.com/styzhang/design-colors#readme",
  "dependencies": {},
  "devDependencies": {
    "sass": "^1.83.1",
    "typescript": "^4.9.5",
    "@types/node": "^16.17.0"
  }
